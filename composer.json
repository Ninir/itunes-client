{
    "name": "adrienbrault/itunes-client",
    "require": {
        "php": ">=5.3.3",
        "guzzle/guzzle": "dev-fix-uri-null-param as 3.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "*"
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "AdrienBrault\\ItunesClient": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/adrienbrault/guzzle"
        }
    ]
}